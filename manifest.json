{
  "name": "Airbnb Sanity",
  "description": "Hide Airbnb listings you don't like (instead of looking through them five times trying to remember what was bad).",
  "version": "0.2.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["http://www.airbnb.com/*", "https://www.airbnb.com/*"],
      "js": ["modify.js"]
    }
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
